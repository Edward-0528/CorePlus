{
  "name": "core-plus",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --profile production",
    "build:android-preview": "eas build --platform android --profile preview",
    "submit:android": "eas submit --platform android --latest",
    "version:bump": "npm version patch && node -e \"const fs=require('fs'); const app=JSON.parse(fs.readFileSync('app.json')); app.expo.android.versionCode++; fs.writeFileSync('app.json', JSON.stringify(app, null, 2));\""
  },
